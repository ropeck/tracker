{
    "version": "0.2.0",
    "configurations": [
        
      {
        "name": "Python Debugger: Current File",
        "type": "debugpy",
        "request": "launch",
        "program": "${file}",
        "console": "integratedTerminal"
      },
      {
        "name": "Run FastAPI (Uvicorn)",
        "type": "python",
        "request": "launch",
        "module": "uvicorn",
        "args": [
          "scripts.logger:app",
          "--reload",
          "--port", "8000"
        ],
        "justMyCode": false,
        "console": "integratedTerminal"
      }
    ]
  }
  